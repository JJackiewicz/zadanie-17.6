doctype html
html
	head
		meta(charset= 'UTF-8')
		title=name
	body
		h1 Sign in
		form(action="/auth/google", method="GET")
			p Login: 
			input(type="text",name="login", placeholder="Login")
			p Password: 
			input(type="text",name="password", placeholder="Password")
		a(href='/auth/google') Sign in